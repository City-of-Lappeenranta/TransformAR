{
  "name": "citizen-webapp",
  "version": "1.0.0-alpha.2",
  "scripts": {
    "ng": "ng",
    "preserve": "npm run generate-icons",
    "serve": "ng serve",
    "prestart": "npm run generate-icons",
    "start": "npx http-server -p 8080 -c-1 dist/citizen-webapp/browser",
    "watch": "ng build --watch --configuration production",
    "prebuild": "npm run generate-icons",
    "prebuild:development": "npm run generate-icons",
    "prebuild:production": "npm run generate-icons",
    "build": "ng build",
    "build:development": "ng build",
    "build:production": "ng build --configuration production",
    "test": "jest",
    "test:ci": "npm run generate-icons; jest --ci --collect-coverage ",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint \"app/**/*.scss\"",
    "prelint:ci": "npm run mkdirp reports",
    "lint:ci": "concurrently \"npm:lint:ts:ci\" \"npm:lint:styles:ci\" -s all",
    "lint:ts:ci": "node prebuild/generate-eslint-reports",
    "lint:styles:ci": "stylelint \"app/**/*.scss\" --formatter json --output-file reports/stylelint-report.json --max-warnings 0",
    "mkdirp": "mkdirp",
    "generate-icons": "node prebuild/generate-svg-icons.ts",
    "prestorybook": "npm run generate-icons",
    "storybook": "ng run citizen-webapp:storybook",
    "build-storybook": "ng run citizen-webapp:build-storybook",
    "sonar-scanner": "sonar-scanner",
    "sentry-cli": "sentry-cli"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.6",
    "@angular/common": "19.1.6",
    "@angular/compiler": "19.1.6",
    "@angular/core": "19.1.6",
    "@angular/forms": "19.1.6",
    "@angular/platform-browser": "19.1.6",
    "@angular/platform-browser-dynamic": "19.1.6",
    "@angular/router": "19.1.6",
    "@angular/service-worker": "19.1.6",
    "@ngx-translate/core": "~16.0.4",
    "@ngx-translate/http-loader": "~16.0.1",
    "@primeng/themes": "~19.0.6",
    "@sentry/angular": "~8.50.0",
    "@types/lodash-es": "~4.17.12",
    "browser-image-compression": "~2.0.2",
    "concurrently": "~9.1.2",
    "eslint-plugin-html": "~8.1.2",
    "google-libphonenumber": "~3.2.40",
    "jest-junit": "~16.0.0",
    "jest-mocha-spec-reporter": "~1.0.17",
    "leaflet": "~1.9.4",
    "lodash-es": "~4.17.21",
    "mkdirp": "~3.0.1",
    "ngx-google-analytics": "^14.0.1",
    "primeng": "~19.0.6",
    "rxjs": "~7.8.0",
    "sonarqube-scanner": "~4.2.6",
    "stylelint": "~16.13.2",
    "stylelint-config-standard-scss": "~14.0.0",
    "stylelint-scss": "~6.10.1",
    "tslib": "~2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.6",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "19.1.6",
    "@angular/compiler-cli": "19.1.6",
    "@sentry/cli": "~2.40.0",
    "@storybook/addon-essentials": "~8.5.0",
    "@storybook/addon-interactions": "~8.5.8",
    "@storybook/addon-links": "~8.5.8",
    "@storybook/angular": "~8.5.8",
    "@storybook/blocks": "~8.5.0",
    "@storybook/test": "~8.5.8",
    "@types/google-libphonenumber": "~7.4.30",
    "@types/jest": "~29.5.12",
    "@types/leaflet": "~1.9.8",
    "@types/node": "~20.11.19",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "~8.57.0",
    "eslint-config-standard-with-typescript": "~43.0.1",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-n": "~16.6.2",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-storybook": "~0.11.2",
    "http-server": "~14.1.1",
    "jest": "~29.7.0",
    "jest-preset-angular": "~14.5.0",
    "jest-sonar-reporter": "~2.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "prettier": "~3.4.2",
    "prettier-eslint": "~16.3.0",
    "shallow-render": "~19.0.1",
    "storybook": "~8.5.0",
    "typescript": "~5.6.3"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "sonar-report.xml",
    "indent": 2
  },
  "engines": {
    "node": "22.12.0",
    "npm": "10.9.0"
  }
}
